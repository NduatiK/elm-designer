{
  "name": "elm-designer",
  "productName": "Elm Designer",
  "version": "0.3.0",
  "description": "A code generator for Elm UI",
  "author": "Andrea Peltrin <designer@passiomatic.com>",
  "repository": "https://github.com/passiomatic/elm-designer",
  "devDependencies": {
    "cssnano": "4.1.10",
    "elm-hot": "1.1.5",
    "node-elm-compiler": "5.0.5",
    "sass": "1.26.10"
  },
  "dependencies": {
    "parcel-bundler": "1.12.4"
  },
  "scripts": {
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html --public-url ./ --out-file index.html",
    "build-no-maps": "parcel build src/index.html --no-source-maps --public-url ./ --out-file index.html",
    "dev-no-debug": "NODE_ENV=production parcel src/index.html",
    "clean": "rm -fR dist"
  }
}
