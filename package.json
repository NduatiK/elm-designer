{
  "name": "elm-designer",
  "productName": "Elm Designer",
  "version": "0.4.0",
  "description": "A code generator for Elm UI",
  "repository": "https://github.com/passiomatic/elm-designer",
  "devDependencies": {
    "@parcel/transformer-elm": "^2.4.1",
    "@parcel/transformer-sass": "^2.4.1",
    "elm": "^0.19.1-5",
    "elm-constants": "^1.0.0",
    "parcel": "^2.4.1"
  },
  "dependencies": {
    "balloon-css": "^1.2.0"
  },
  "scripts": {
    "dev": "parcel src/index.html",
    "constants": "elm-constants",
    "build": "parcel build src/index.html --public-url ./",
    "build-no-maps": "parcel build src/index.html --no-source-maps --public-url ./",
    "dev-no-debug": "NODE_ENV=production parcel src/index.html",
    "clean": "rm -fR dist"
  }
}
